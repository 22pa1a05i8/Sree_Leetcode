class Solution {
    public int[] getConcatenation(int[] nums) {
        int[] arr1=new int[nums.length * 2];
        int j=0;
        for (int i=0;i<nums.length;i++){

            arr1[j]=nums[i];
            j+=1;
        }
        for (int i=0;i<nums.length;i++){

            arr1[j]=nums[i];
            j+=1;
        }
        return arr1;
    }
}
